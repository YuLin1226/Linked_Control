function [new_pos] = window_planner(av, aw, resolution, dt)
    v_max = av * dt;
    v_min = 0;
    v_num = (v_max - v_min) / resolution;
    
    w_max = aw * dt;
    w_min = -aw * dt;
    w_num = (w_max - w_min) / resolution;
    
    v_applicable = [];
    for i = 0: v_num
        v_applicable = [v_applicable, ]
    end
    
end